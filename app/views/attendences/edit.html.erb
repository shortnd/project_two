<h2>Edit Your Account</h2>
<%= form_for @attendence do |f| %>
  <%= f.label :fullName %>
  <%= f.text_field :fullName %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :chapter %>
  <%= f.text_field :chapter %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :phone %>
  <%= f.text_field :phoneNumber %>

  <%#= f.select(:event, options_for_select(@event_season)) %>

  <%= f.submit "Submit" %>
<% end %>

<%#= form_for @attendence, method: :delete do |f| %>
  <%#= f.submit "Delete!", class: "delete" %>
<%# end %>
