<%= form_for [@event, @attendence] do |f| %>
  <%= f.label :fullName %>
  <%= f.text_field :fullName %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :chapter %>
  <%= f.text_field :chapter %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :phone %>
  <%= f.text_field :phoneNumber %>

  <%= f.submit "Submit" %>
<% end %>
